{
    "files": [
      "assets/ts/main.ts"
    ],
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "sourceMap": true,
      "removeComments": true,
      "strict": true,
      "noImplicitAny": true,
      "strictFunctionTypes": true,
      "strictBindCallApply": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "typeRoots": [
        "../@types",
        "../../node_modules/@types"
      ],
      "esModuleInterop": true
    }
  }
  